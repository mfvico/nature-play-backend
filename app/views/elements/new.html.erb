<%= form_for @element do |f| %>

  <%= f.label :latitude %>
  <%= f.text_field :latitude %>
  <%= f.label :longitude %>
  <%= f.text_field :longitude %>
  <%= f.label :description %>
  <%= f.text_area :description %>
  <%= f.label :category %>
  <%= f.text_field :category %>
  <%= f.label "Park ID (optional)" %>
  <%= f.text_field :park_id %>

  <%= f.submit %>

<% end %>
